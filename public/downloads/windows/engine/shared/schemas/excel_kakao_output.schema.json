{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "excel_kakao_output",
  "type": "object",
  "required": [
    "action",
    "result"
  ],
  "properties": {
    "action": {
      "type": "string",
      "enum": [
        "create_group",
        "add_members",
        "send_message",
        "noop"
      ]
    },
    "result": {
      "type": "object",
      "required": [
        "text"
      ],
      "properties": {
        "text": {
          "type": "string",
          "minLength": 1
        }
      },
      "additionalProperties": true
    },
    "warnings": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "additionalProperties": true
}